  "engines": {
    "node": "6.9.5",
    "npm": "3.10.7"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "dev": "npm run build-dev && npm run start",
    "test": "mocha --recursive --compilers jsx:babel-core/register",
