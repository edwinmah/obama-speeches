  "devDependencies": {
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.1",
    "mocha": "^3.0.1",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.18.2",
